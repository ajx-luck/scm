534:              /****读8bit
535:              注意：本程序采用双向IO口，对于需要配置端口特性的MCU需要注意
536:              在发送指令时配置为输出模式;
537:              接受ACK为输入模式；
538:              读数据位输入模式；
539:              **************************/
540:              u8t read_8bit()
541:              {
542:              	uchar dat,i;
543:              	SDA=1;
544:              	dat=0;
545:              	for(i=0;i<8;i++)
546:              	{
547:              	SCL=1;                        //时钟上沿
548:              	nop;
549:              	nop;
550:              	nop;
551:              	dat<<=1;
552:              	if(SDA)
553:              	 dat++;
554:              	SCL=0;
555:              	nop;
556:              	nop;
557:              	nop;
558:              	nop;
559:              	}
560:              	SDA=0;			    //ACK信号
561:              	nop;
562:              	nop;
563:              	nop;
564:              	SCL=1;
565:              	nop;
566:              	nop;
567:              	nop;
568:              	nop;
569:              	SCL=0;
570:              	nop;
571:              	
572:              	return dat ;
573:              } 
