uint8 keyClick = 0;
uint8 keyNub = 0;
uint8 keyCount = 0;
char numArray[10] = {0x3F, 0x06,0x5B,0x4F,0x66,0x6D,0x7D,0x07,0x7F,0x67};


void initConfig();
void Sleep_Mode();
void refreshNub();
