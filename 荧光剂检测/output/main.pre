void interrupt Timer0_Isr() {
if (T0IF)
{


TMR0 = 150;

if(++intCount >= 100)
{
intFlag = 1;
if(++count500ms >= 100)
{
count500ms = 0;
timeCount++;
if(nubShowTime > 0)
{
--nubShowTime;
}
}
}

T0IF = 0;
}

if (RBIF) {
RBIF = 0;
}
}


void main()
{
initConfig();
for(;;)
{
asm("clrwdt");
refreshNub();
if(intFlag == 0)
continue;
intFlag = 0;

keyCtr();

TRISB |= 0x04;
ANSEL = 0x00;
ANSELH = 0x01;

uint16 res = ADC_AVG(0x0F,8);
uint16 av = 6000/res;
uint16 res2 = 0;
for(int i=0;i<10;i++)
{
refreshNub();
}

# 104
res2 = ADC_AVG(0x08,8);
if(res2)
{

res = res2 * av;

nubShowTime = 20;
}
nubShowTime = 20;

}
}


void keyCtr()
{
keyClick = keyRead(0x18&(~PORTB));
if(keyClick > 0)
{
if(keyNub & 0x08)
{

PORTB ^= 0x20;
}
else
{

PORTE |= 0x01;
nubShowTime = 20;
count500ms = 0;
}


}
}


void refreshNub()
{
if(nubShowTime == 0)
{
setInput();
return;
}
if(++refreshCount >= 10)
{
refreshCount = 0;
tempbaiweiNum = ygNub/100;
tempshiweiNum = (ygNub%100)/10;
tempgeweiNum = ygNub%10;
}
low1(numArray[tempbaiweiNum],numArray[tempshiweiNum],numArray[tempgeweiNum],1,1);
low2(numArray[tempbaiweiNum],numArray[tempshiweiNum],numArray[tempgeweiNum],1);
low3(numArray[tempbaiweiNum],numArray[tempshiweiNum],numArray[tempgeweiNum],1);
low4(numArray[tempbaiweiNum],numArray[tempshiweiNum],numArray[tempgeweiNum],1,1);

# 168
DelayXms(1);
switch(nubStep)
{
case 0:
WU();
break;
case 1:
RUO();
break;
case 2:
ZHONG();
break;
case 3:
QIANG();
break;
}
DelayXms(3);
}

unsigned int ADC_AVG(unsigned char adch,unsigned char adtime)
{
unsigned int adsum = 0;
for(char i = 0; i< adtime;i++)
{
adsum+=ADC_Sample(adch);
refreshNub();
}
return adsum/adtime;
}


void initConfig()
{
COMEN = 0x00;
PORTA = 0x00;
PORTB = 0x00;
PORTC = 0x00;
TRISA = 0;
TRISB = 0x18;
TRISC = 0;
WPUA = 0x00;
WPUB = 0x18;
WPUC = 0x00;
WPUE = 0x00;
PORTA = 0x00;
PORTB = 0x00;
PORTC = 0x00;
IOCB = 0x00;


asm("nop");
asm("clrwdt");
INTCON = 0;
OSCCON = 0X71;
OPTION_REG = 0x06;

TMR0 = 0;
INTCON = 0xE0;
}


void Sleep_Mode() {
INTCON = 0;

OPTION_REG = 0;

WPUB = 0x18;


IOCB = 0x18;
RBIE = 1;
GIE = 0;

ADCON0 = 0;

OSCCON = 0X70;

PORTB;

asm("clrwdt");

asm("sleep");

asm("nop");
OSCCON = 0X71;
IOCB = 0x00;
}

char keyRead(char KeyStatus)
{
if (KeyStatus)
{
keyNub = KeyStatus;
keyCount++;
}
else
{
if(keyCount >= 8)
{
keyCount = 0;
return 1;
}
keyCount = 0;
keyNub = 0;
}
return 0;
}
